{
  "name": "AniVA",
  "version": "1.3.0",
  "private": true,
  "dependencies": {
    "react": "^16.12.0",
    "react-content-loader": "^4.3.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router": "^6.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-snap": "^1.23.0",
    "typescript": "^3.7.3"
  },
  "devDependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-snapshot": {
    "include": [
      "/va/95185"
    ],
    "exclude": [
      "/va/undefined"
    ],
    "snapshotDelay": 5000
  },
  "puppeteerArgs": [
    "--no-sandbox",
    "--disable-setuid-sandbox"
  ],
  "puppeteer": {
    "waitUntil": "networkidle0"
  }
}
